.container {
  margin: 0;
  padding: 0;
  height: 560px;
  background-color: #f7f7f7;
}
.container .head {
  position: relative;
  height: 200rpx;
}
.container .head image {
  position: absolute;
  top: 18%;
  left: 40%;
  width: 150rpx;
  height: 150rpx;
  border-radius: 125%;
  border: 1px solid #ccc;
  overflow: hidden;
}
.container .head .login {
  margin: 0;
  width: 100%;
  height: 200rpx;
  background-color: #fff;
}
.container .head .login text {
  font-size: 20px;
  font-weight: 400;
  color: #2c2c2c;
  line-height: 200rpx;
}
.container .body .about-me {
  width: 100%;
  margin-top: 15rpx;
  color: #2c2c2c;
}
.container .body .kefu {
  width: 100%;
  padding: 0;
  margin: 0;
  text-align: left;
  font-weight: 400;
}
